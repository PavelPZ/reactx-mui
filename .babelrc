{
  "presets": [ "babel-preset-expo" ],
  "env": {
    "development": {
      "plugins": [ "transform-react-jsx-source" ]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "reactx-mui/test": "./lib-native/test",
          "reactx-mui/current": "./lib-native/native",
          "reactx-mui/native": "./lib-native/native",
          "reactx-mui/common": "./lib-native/common"
        }
      }
    ]
  ]
}
