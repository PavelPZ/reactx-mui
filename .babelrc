{
  "presets": [ "babel-preset-expo" ],
  "env": {
    "development": {
      "plugins": [ "transform-react-jsx-source" ]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "reactx-mui": "./lib-native",
          "reactx-mui/current": "./lib-native/native"
        }
      }
    ]
  ]
}
